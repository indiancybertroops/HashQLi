echo ""
PS3='root@HashQLi:'
options=( "Single Hash Decrypter" "Multi Hash Decrypter" "MD5 Decrypter With New Api" "Exit"
)
select opt in "${options[@]}"
do
    case $opt in
        "Single Hash Decrypter")
            clear
N='\033[0m'
R='\033[0;31m'
G='\033[0;32m'
O='\033[0;33m'
  echo -e "
${O}██   ██  █████  ███████ ██   ██  ██████  ██      ██ 
${N}██   ██ ██   ██ ██      ██   ██ ██    ██ ██      ██ 
